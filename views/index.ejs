<!DOCTYPE html>
<html>
<head>
    <title>Users</title>
</head>
<body>

<h1>Users List</h1>

<a href="/add">Add User</a>

<table border="1" cellpadding="10">
    <tr>
        <th>Name</th>
        <th>Gender</th>
        <th>Age</th>
        <th>Country</th>
    </tr>

    <% users.forEach(user => { %>
        <tr>
            <td><%= user.name %></td>
            <td><%= user.gender %></td>
            <td><%= user.age %></td>
            <td><%= user.country %></td>
        </tr>
    <% }) %>
</table>

</body>
</html>
